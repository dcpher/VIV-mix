<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1">
    
	<link rel="shortcut icon" href="/favicon.png">
	{{ "assets/css/style.css" | asset_url | stylesheet_tag }}
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
  
    
	{% block header %}
	<title>{% block title %}Default{% endblock %}</title>
	{% endblock header %}
  
    {% include "header-scripts" %}

</head>
<body>
  
  
    {% include "header-navigation" %}
  <div class="off-canvas-wrap" data-offcanvas>
  <div class="inner-wrap">
    
    <nav class="tab-bar">

      <section class="left tab-bar-logo">
         <a href="#"><img class="logo x2" src="/Media/VIV/main/vivamos-logo-inverse.png" /></a>
      </section>
      
      <section class="right-small hide-for-large-up">
        <a class="right-off-canvas-toggle menu-icon" href="#"><span></span></a>
      </section>
      
      <section class="right tab-bar-section show-for-medium-up">
        <ul class="no-bullet inline-list right">
          <li class=""><a href="#">Seguro Viagem</a></li>
          <li class="show-for-large-up"><a href="#">Planos e Cobertura</a></li>
          <li class="show-for-large-up"><a href="#">Emergências</a></li>
          <li class="show-for-large-up"><a href="#">Aviso de Sinistro</a></li>
          <li class=""><a href="#">Contato</a></li>
          <li class="sign-in"><a href="#">Área do Cliente</a></li>
        </ul>
      </section>


    </nav>

    <aside class="right-off-canvas-menu">
      <ul class="off-canvas-list">
        <li class="text-center"><a href="#"><img class="logo x2" src="/Media/VIV/main/vivamos-logo-inverse.png" /></a></li>
        <li><a href="#">Seguro Viagem</a></li>
        <li><a href="#">Planos e Cobertura</a></li>
        <li><a href="#">Emergências</a></li>
        <li><a href="#">Aviso de Sinistro</a></li>
        <li><a href="#">Contato</a></li>
        <li class="sign-in"><a href="#">Área do Cliente</a></li>
      </ul>
    </aside>
    
    <section class="main-section">
      <!-- content goes here -->

  
	{% block content %}
		Main content
	{% endblock content %}
  
      

    {% include "footer" %}
    {% include "footer-scripts" %}
    </section>

    <a class="exit-off-canvas"></a>

  </div>
</div>


</body>
</html>
