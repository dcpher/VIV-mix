<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1">
    
	<link rel="shortcut icon" href="/favicon.png">
	{{ "assets/css/style.css" | asset_url | stylesheet_tag }}
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
  
    
	{% block header %}
	<title>{% block title %}Default{% endblock %}</title>
	{% endblock header %}
  
    {% include "header-scripts" %}

</head>
<body>
  
<nav class="top-bar" data-topbar role="navigation">
  <ul class="title-area">
    <li class="name">
      <h1><a href="#"><img class="logo x2" src="/Media/Default/main/vivamos-logo-inverse.png" /></a></h1>
    </li>
     <!-- Remove the class "menu-icon" to get rid of menu icon. Take out "Menu" to just have icon alone -->
    <li class="toggle-topbar menu-icon"><a href="#"><span>Menu</span></a></li>
  </ul>

  <section class="top-bar-section">
    <!-- Right Nav Section -->
    <ul class="right">
      <li class=""><a href="#">Seguro Viagem</a></li>
      <li class=""><a href="#">Planos e Cobertura</a></li>
      <li class="hide-for-medium-only"><a href="#">Emergências</a></li>
      <li class=""><a href="#">Aviso de Sinistro</a></li>
      <li class=""><a href="#">Contato</a></li>
      <li class="sign-in"><a href="#">Área do Cliente</a></li>
    </ul>
  </section>
</nav>
  
	{% block content %}
		Main content
	{% endblock content %}
  
      

    {% include "footer" %}
    {% include "footer-scripts" %}


</body>
</html>
